package EDGE;

public class stringConcatination {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "ajit";
		StringBuffer b = new StringBuffer(s);
		b.reverse();
		s=b.toString();
		System.out.println(s);
		String t = "";
		t=t+s.charAt(0);
		System.out.println(t);

	}

}
